{
  "popclip version": 3783,
  "identifier": "com.pilotmoon.popclip.extension.timestamp",
  "name": "Timestamp",
  "icon": "clock.png",
  "requirements": [
    "paste"
  ],
  "regex": "(?s)^$",
  "module": "timestamp.js",
  "after": "paste-result",
  "description": "Paste the current date and time.",
  "options": [
    {
      "label": "Date format",
      "type": "multiple",
      "valueLabels": [
        "2021-10-04",
        "2021/10/04",
        "20211004",
        "4/10/2021",
        "10/4/2021"
      ],
      "values": [
        "Y-m-d",
        "Y/m/d",
        "Ymd",
        "d/m/Y",
        "m/d/Y"
      ],
      "identifier": "date"
    },
    {
      "label": "Time format",
      "type": "multiple",
      "values": [
        "H:i:s",
        "H:i",
        ""
      ],
      "valueLabels": [
        "12:15:01",
        "12:15",
        "None"
      ],
      "identifier": "time"
    }
  ]
}
