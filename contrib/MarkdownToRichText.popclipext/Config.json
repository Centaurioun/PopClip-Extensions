{
    "identifier": "com.pilotmoon.popclip.extension.markdown-to-rtf",
    "name": "MD to Rich Text",
    "icon": "RTF",
    "longName": "Markdown to Rich Text",
    "description": "Convert Markdown source to Rich Text content.",
    "popclipVersion": 3835,
    "macosVersion": "12.0",    
    "module": "mdrtf.js",
    "captureRtf": true,
    "captureHtml": true,
    "options": [
        {
            "identifier": "paras",
            "type": "multiple",
            "label": "Paragraph Separation",
            "values": ["spacing", "newlines"],
            "valueLabels": ["0.5x Spacing", "Blank Line"]
        }
    ]
}