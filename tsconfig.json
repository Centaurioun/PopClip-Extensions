{
    "typedocOptions": {        
        "entryPoints": ["./popclip.d.ts"],
        "name": "PopClip Extensions JavaScript Reference",    
        "out": "./docs",
        "disableSources": false,
        "sort": ["alphabetical"],
        "excludeExternals": true,
        "readme": "README-js.md",
        "excludeNotDocumented": true
    },    
    "compilerOptions": {         
        "strictNullChecks": true,       
        "target": "ES5",
        "lib": ["ES5"],
        "skipLibCheck": true,
        "module": "commonjs",
        "moduleResolution": "node"
    },
    "include": ["popclip.d.ts", "./source", "./lib"],
    "ts-standard": {
        "ignore": [
        "**/*.js"        
        ]
    }
}